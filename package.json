{
  "name": "openqed-cli",
  "version": "0.1.0",
  "description": "AI session provenance for commits, PRs, and reviews",
  "type": "module",
  "bin": { "openqed": "./dist/index.js" },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": { "node": ">=20.0.0" },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "better-sqlite3": "^11.0.0",
    "simple-git": "^3.27.0",
    "yaml": "^2.7.0",
    "chalk": "^5.4.0",
    "ora": "^8.0.0",
    "strip-ansi": "^7.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^3.0.0",
    "typescript": "^5.7.0",
    "@types/better-sqlite3": "^7.0.0",
    "@types/node": "^22.0.0"
  },
  "license": "MIT"
}
